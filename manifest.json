{
    "manifest_version": 3,

    "version": 1.0,

    "name": "PocketBookmarks",

    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },

    "default_locale": "en",

    "description": "Save the tabs you're exploring for easy access",

    "author": "Mohieb",
    
    "permissions": [
        "tabs"
    ]
}